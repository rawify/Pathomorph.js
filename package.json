{
    "name": "pathomorph",
    "title": "Pathomorph",
    "version": "0.0.4",
    "description": "The RAW library to create SVG path elements out of geometric primitives",
    "homepage": "https://github.com/rawify/Pathomorph.js",
    "bugs": "https://github.com/rawify/Pathomorph.js/issues",
    "keywords": [
        "svg",
        "path",
        "primitives",
        "arc",
        "line",
        "rect",
        "polyline",
        "arrow",
        "partial path"
    ],
    "private": false,
    "main": "./dist/pathomorph.js",
    "module": "./dist/pathomorph.mjs",
    "browser": "./dist/pathomorph.min.js",
    "unpkg": "./dist/pathomorph.min.js",
    "types": "./pathomorph.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./pathomorph.d.mts",
                "default": "./dist/pathomorph.mjs"
            },
            "require": {
                "types": "./pathomorph.d.ts",
                "default": "./dist/pathomorph.js"
            },
            "browser": {
                "types": "./pathomorph.d.ts",
                "default": "./dist/pathomorph.min.js"
            },
            "default": {
                "types": "./pathomorph.d.ts",
                "default": "./dist/pathomorph.js"
            }
        },
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "<4.7": {
            "*": [
                "pathomorph.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git@github.com:rawify/Pathomorph.js.git"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/rawify"
    },
    "author": {
        "name": "Robert Eisele",
        "email": "robert@raw.org",
        "url": "https://raw.org/"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "crude-build Pathomorph"
    },
    "devDependencies": {
        "crude-build": "^0.1.2"
    },
    "dependencies": {
        "@rawify/vector2": "^0.0.6"
    }
}